{
    "name": "react-f-to-c",
    "displayName": "react-f-to-c",
    "description": "Convert functional React component to class component",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.convertComponent"
    ],
    "main": "./src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.convertComponent",
                "title": "react-f-to-c"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test",
        "watch": "webpack --watch",
        "build": "webpack"
    },
    "devDependencies": {
        "@babel/generator": "^7.2.2",
        "@babel/parser": "^7.2.2",
        "@babel/traverse": "^7.2.3",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "eslint": "^4.11.0",
        "typescript": "^3.1.4",
        "vscode": "^1.1.25"
    },
    "dependencies": {
        "babel-types": "^6.26.0"
    }
}
